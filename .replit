modules = ["dotnet-7.0", "dotnet-8.0"]
[agent]
expertMode = true

[nix]
channel = "stable-25_05"
packages = ["mono", "gtk3", "xorg.libX11", "xorg.libXext", "xorg.libXrender", "tigervnc"]

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Car Dashboard App"

[[workflows.workflow]]
name = "Car Dashboard App"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "DISPLAY=:0 mono \"bin/Debug/CS 363 Car App.exe\""

[workflows.workflow.metadata]
outputType = "vnc"

[[ports]]
localPort = 37939
externalPort = 80
